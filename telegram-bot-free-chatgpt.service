[Unit]
Description=Telegram Bot Free chatGPT Service
After=network.target

[Service]
Type=simple
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/freechatgpt
ExecStart=/home/ubuntu/.tb1/bin/python3 /home/ubuntu/freechatgpt/tb.py
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
